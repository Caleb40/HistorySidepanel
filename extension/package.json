{
  "name": "history-sidepanel-extension",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/sidepanel/main.tsx",
  "module": "src/sidepanel/main.tsx",
  "scripts": {
    "dev": "bun --hot src/sidepanel/main.tsx",
    "build": "bun build src/sidepanel/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/sidepanel/main.tsx"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/chrome": "^0.1.24",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "paths": {
    "@/*": [
      "src/*"
    ]
  }
}
